<div class="github">

  <div class="github-navbar">
    <div class="issues-tab">
      <a href="#" class="tab-underlined active"><%= "issues".upcase %></a>
      <i id="issues-arrow" class="fas fa-sort-up"></i>
    </div>
    <div class="pull-requests-tab">
      <a href="#" class="tab-underlined"><%= "pull requests".upcase %></a>
      <i id="pull-requests-arrow" class="fas fa-sort-up"></i>
    </div>
  </div>

  <%= 10.times do |issuecards| %>
    <a href="https://github.com/" target="_blank">
      <div class="github-issue-card">
        <p id="issue-repo">Repo: GoogleChrome/myrepo</p>
        <p id="issue-number">Issue #: <%= rand(0..2000) %></p>
        <p id="issue-description">Lorem ipsum dolor sit amet, consec tetur adipiscing elit.</p>
      </div>
    </a>
  <% end %>

  <%= 5.times do |prcards| %>
    <a href="https://github.com/" target="_blank">
      <div class="github-pull-request-card">
        <p id="pr-repo">Repo: GoogleChrome/team_project_repo</p>
        <p id="pr-number">Pull request #: <%= rand(0..2000) %></p>
        <p id="pr-description">You got a pull resquest from your team member.</p>
      </div>
    </a>
  <% end %>
</div>
